RUN apt-get -qq update \
    && apt-get -qqy install \
    autoconf \
    automake \
    build-essential \
    curl \
    libtool \
    libunwind8 \
    unzip \
    make \
    g++ \
    uuid-dev \
    libcurl4-openssl-dev \
    libc6-dev-i386 \
    libpcre3-dev \
    libssl-dev \
    zlib1g-dev
